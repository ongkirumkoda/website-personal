{
    "name": "siraga/siraga",
    "description": "SIRAGA - Sistem Pencatatan Tumbuh Kembang Anak",
    "type": "project",
    "keywords": [
        "healthcare",
        "child-growth",
        "monitoring",
        "php",
        "mvc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "SIRAGA Development Team",
            "email": "dev@siraga.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "phpmailer/phpmailer": "^6.8",
        "google/apiclient": "^2.15",
        "twilio/sdk": "^7.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "dompdf/dompdf": "^2.0",
        "firebase/php-jwt": "^6.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Siraga\\": "app/"
        },
        "files": [
            "app/helpers/functions.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"copy('.env.example', '.env');\""
        ],
        "migrate": "php database/migrations/migrate.php",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
